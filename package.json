{
  "name": "node-red-contrib-br-validations",
  "version": "0.1.1",
  "description": "Validates some Brazilian documents such as CPF, CNPJ and PIS - based on the br-validations library",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sciensa/node-red-contrib-br-validations.git"
  },
  "scripts": {
    "test:unit": "mocha --opts test/unit/mocha.opts test/",
    "postinstall": "npm --prefix ./node_modules/node-red install ./node_modules/node-red"
  },
  "keywords": [
    "node-red",
    "CPF",
    "CNPJ",
    "PIS",
    "PASEP",
    "PIS/PASEP",
    "brazilian",
    "documents",
    "validations",
    "br-validations"
  ],
  "node-red": {
    "nodes": {
      "br-validations": "br/validations.js"
    }
  },
  "author": "Sciensa Consulting <sciensa@sciensa.com.br> (http://github.com/sciensa)",
  "contributors": [
    "Aline Castro <aline.castro@sciensa.com.br> (http://github.com/alinecastro33)",
    "Rogério R. Alcântara <rogerio.alcantara@sciensa.com.br> (http://github.com/roalcantara)",
    "Valter Junior <valter.junior@sciensa.com.br> (http://github.com/valterjnr)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sciensa/node-red-contrib-br-validations/issues"
  },
  "homepage": "https://github.com/sciensa/node-red-contrib-br-validations#readme",
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "mocha": "^9.1.3",
    "nock": "^13.2.1",
    "node-red": "git+ssh://git@github.com/node-red/node-red#2.1.3",
    "path": "^0.12.7",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "br-validations": "^0.3.1"
  }
}
